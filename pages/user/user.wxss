/* user/user.wxss */
page{
  background: #f2f2f2;
}
button.user_info{
  margin: 0rpx;
  padding: 0rpx;
  width: 100%;
  height: 160rpx;
  margin-top: 20rpx;
  background: #ffffff;
}
.user_info image{
  float: left;
  width: 105rpx;
  height: 105rpx;
  display: block;
  margin-top: 27.5rpx;
  margin-left: 47rpx;
}
.user_info text{
  float: left;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 30rpx;
  margin-top:75rpx;
  margin-left: 35rpx; 
  font-weight: bold;
}
.user_item{
  width: 100%;
  height: 94rpx;
  background: #ffffff;
  border-bottom: 1rpx solid #f2f2f2;
  color: #292929;
}
.user_item .iconfont{
  margin-top: 20rpx;
  margin-left: 46rpx;
  float: left;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: #ffffff;
  font-size: 38rpx;
  border-radius: 50%;
  background: #F2CD0B;
}
.user_item  text{
  height: 26rpx;
  line-height: 26rpx;
  float: left;
  margin-left: 20rpx;
  font-size: 26rpx;
  margin-top: 33rpx;
}
.user_item .more{
  float: right;
  width: 23rpx;
  height: 23rpx;
  margin-top: 36rpx;
  margin-right: 53rpx;
}
navigator{
  width: 100%;
}